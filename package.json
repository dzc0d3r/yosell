{
  "name": "yosell-monorepo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "pnpm --filter @yosell/db run generate",
    "db:push": "pnpm --filter @yosell/db run push",
    "db:studio": "pnpm --filter @yosell/db run studio",
    "db:seed": "pnpm --filter @yosell/db run seed",
    "db:migrate": "pnpm --filter @yosell/db run migrate",
    "db:lint": "pnpm --filter @yosell/db run lint"

  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.39.0",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  }
}
